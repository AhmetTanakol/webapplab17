				},
				"city": {
					"type": "string"
				}
			}
		},
		"dateAndTimeOfParty": {
			"type": "string",
			"format": "date-time"
		},
		"timeOfArrival": {
			"type": "string",
			"format": "time"
		},
		"streetnumber": {
			"type": "string",
			"maxLength": 4
		},
		"postalCode": {
			"type": "string",
			"maxLength": 5
		}
	}
